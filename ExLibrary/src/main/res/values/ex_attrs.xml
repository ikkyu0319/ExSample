<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ExDecorView">
        <attr name="titleFloat" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="LinearListView">
        <attr name="dividerDimension" format="dimension" />
        <attr name="dividerDrawable" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="RatingView">
        <attr name="rating" format="integer" />
        <attr name="numStar" format="integer" />
        <attr name="lightDrawable" format="reference" />
        <attr name="halfDrawable" format="reference" />
        <attr name="darkDrawable" format="reference" />
        <attr name="spaceStar" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ProgressView">
        <attr name="progress_src" format="reference|color" />
    </declare-styleable>


    <declare-styleable name="Switch">
        <attr name="sw_trackSize" format="reference|dimension"/>
        <attr name="sw_trackColor" format="reference|color"/>
        <attr name="sw_trackCap" format="integer">
            <enum name="butt" value="0x00000000" />
            <enum name="round" value="0x00000001" />
            <enum name="square" value="0x00000002" />
        </attr>
        <attr name="sw_thumbColor" format="reference|color"/>
        <attr name="sw_thumbRadius" format="reference|dimension"/>
        <attr name="sw_thumbElevation" format="reference|dimension"/>
        <attr name="sw_animDuration" format="reference|integer"/>
        <attr name="sw_interpolator" format="reference"/>
        <attr name="android:gravity" />
        <attr name="android:checked" />
    </declare-styleable>


</resources>